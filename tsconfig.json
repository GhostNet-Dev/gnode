{
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "baseUrl": "./", /* Specify the base directory to resolve non-relative module names. */
    "paths": {
      "@Commons/*": [ "src/common/*" ],
      "@Models/*": [ "src/models/*" ],
      "@Glibs/*": [ "src/gsdk/src/*" ],
    },
  },
  "include": [
    "src"
  ]
}

